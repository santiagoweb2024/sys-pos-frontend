{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Start:Dev",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": ["run", "start:dev"],
      "cwd": "${workspaceFolder}/server",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/server/node_modules/**"
      ],
      "outFiles": ["${workspaceFolder}/**/*.js"]
    }
  ]
}
